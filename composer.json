{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^3.7",
        "beberlei/doctrineextensions": "^1.3",
        "bluepsyduck/symfony-process-manager": "^1.3",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "dompdf/dompdf": "^2.0",
        "endroid/qr-code-bundle": "^4.2",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "gedmo/doctrine-extensions": "^3.11",
        "james-heinrich/getid3": "^1.9",
        "knplabs/knp-time-bundle": "^1.20",
        "league/csv": "^9.8",
        "league/flysystem": "^3.12",
        "league/flysystem-aws-s3-v3": "^3.12",
        "liip/imagine-bundle": "^2.10",
        "nesbot/carbon": "^2.66",
        "nyholm/psr7": "^1.5",
        "oneup/flysystem-bundle": "^4.6",
        "php-translation/symfony-bundle": "^0.12.8",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.16",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/mailgun-mailer": "6.2.*",
        "symfony/mercure-bundle": "^0.3.5",
        "symfony/messenger": "6.2.*",
        "symfony/mime": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.2.*",
        "symfony/process": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/security-csrf": "6.2.*",
        "symfony/sendgrid-mailer": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/string": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/ux-chartjs": "^2.7",
        "symfony/ux-notify": "^2.7",
        "symfony/ux-swup": "^2.7",
        "symfony/ux-turbo": "^2.7",
        "symfony/ux-turbo-mercure": "^2.6",
        "symfony/ux-twig-component": "^2.7",
        "symfony/validator": "6.2.*",
        "symfony/web-link": "6.2.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.2.*",
        "symfonycasts/reset-password-bundle": "^1.17",
        "symfonycasts/verify-email-bundle": "^1.13",
        "tetranz/select2entity-bundle": "^3.1",
        "twig/cssinliner-extra": "^3.5",
        "twig/extra-bundle": "^3.5",
        "twig/inky-extra": "^3.5",
        "twig/intl-extra": "^3.5",
        "twig/markdown-extra": "^3.5",
        "twig/string-extra": "^3.5",
        "twig/twig": "^2.12|^3.0",
        "webimpress/safe-writer": "^2.2",
        "xiidea/easy-audit": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "docker": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.15.12",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/debug-bundle": "6.2.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "6.2.*",
        "symfony/web-profiler-bundle": "6.2.*"
    }
}
